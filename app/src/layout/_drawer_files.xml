<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<RelativeLayout
		android:id="@+id/relativeLayout"
		android:layout_width="251dp"
		android:layout_height="match_parent"
		android:background="#000000"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/particleView"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:padding="8dp"
			android:orientation="vertical" />
		<LinearLayout
			android:id="@+id/ui1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<ScrollView
				android:id="@+id/vscroll2"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear14"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear15"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/imageview6"
								android:layout_width="50dp"
								android:layout_height="50dp"
								android:src="@drawable/ic_splash"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/textview13"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="PSP TOOLS"
								android:textSize="16sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/gamedownload"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:text="Game download"
							android:textSize="14sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/gamedownload_path"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/download"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/iconfolder_psptools"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_game"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Games"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/emu"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:text="Run Emu"
							android:textSize="14sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/ppsspp"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconppsspp"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/iconfolder_psptools"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_psp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Run ppsspp emu"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview17"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:text="Gitub App"
							android:textSize="14sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/githubs"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/icongithub"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/iconfolder_psptools"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_git"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Get Github"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview25"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="path Data Psp room"
							android:textSize="14sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/linear17"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconpathdownload"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/textview26"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="download Paths Psp"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview19"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:text="Other"
							android:textSize="14sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/setting"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/setting_icon"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_sti"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Setting App"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/about"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/abouticon"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_ab"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="about"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/about_your_mobile"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconabout_your_mobile"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_ab2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="about moblie"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/Telegram"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconTelegram"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_tel"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Get Telegram"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/shell"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconshell"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_cdm"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Shell eum"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/keyboardinstall"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconkeyboard"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/textview24"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Keyboard Psp Tools"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview23"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginTop="5dp"
							android:layout_marginBottom="5dp"
							android:padding="8dp"
							android:text="App Exit"
							android:textSize="12sp"
							android:textColor="#FF5722" />
						<LinearLayout
							android:id="@+id/exit"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/iconexit"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:src="@drawable/default_image"
								android:scaleType="centerInside" />
							<TextView
								android:id="@+id/drawer_by"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="Exit"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
		</LinearLayout>
	</RelativeLayout>
</LinearLayout>
