<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bg"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:background="#E0E0E0"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/textview1"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="select color and theme app"
				android:textSize="14sp"
				android:textColor="#000000" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<ScrollView
				android:id="@+id/vscroll1"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/linear5"
						android:layout_width="match_parent"
						android:layout_height="90dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/pink"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#E91E63"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/red"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#F44336"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Purple"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#9C27B0"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/darkviolet"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#673AB7"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Darkblue"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#3F51B5"
							android:orientation="horizontal" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear11"
						android:layout_width="match_parent"
						android:layout_height="90dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/blue"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#2196F3"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/blue2"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#03A9F4"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/sky"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#00BCD4"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Cyan"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#009688"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/green"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#4CAF50"
							android:orientation="horizontal" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear17"
						android:layout_width="match_parent"
						android:layout_height="90dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/litegreen"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#8BC34A"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Lemon"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#CDDC39"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/yellow"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#FFEB3B"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Brightorange"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#FFC107"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Orange"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="5dp"
							android:layout_marginRight="5dp"
							android:padding="8dp"
							android:background="#FF9800"
							android:orientation="horizontal" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear23"
						android:layout_width="match_parent"
						android:layout_height="90dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/deepOrange"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#FF5722"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Brown"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#795548"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Gray"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#9E9E9E"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Grayblue"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#607D8B"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/Black"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#000000"
							android:orientation="horizontal" />
						<LinearLayout
							android:id="@+id/White"
							android:layout_width="50dp"
							android:layout_height="50dp"
							android:layout_marginLeft="4dp"
							android:layout_marginRight="4dp"
							android:padding="8dp"
							android:background="#FFFFFF"
							android:orientation="horizontal" />
					</LinearLayout>
				</LinearLayout>
			</ScrollView>
		</LinearLayout>
		<TextView
			android:id="@+id/mis"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="3dp"
			android:layout_marginTop="5dp"
			android:padding="8dp"
			android:gravity="right"
			android:text="OK"
			android:textSize="14sp"
			android:textColor="#000000"
			android:layout_gravity="right" />
	</LinearLayout>
</LinearLayout>
